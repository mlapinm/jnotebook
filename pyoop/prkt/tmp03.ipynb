    "import re\n",
    "# очистить ссылку\n",
    "def clear_tagparams( text ):\n",
    "    return re.sub( r'<a.*?(href=\\\"[^\\\"]*?\\\")[^>]*?>', \n",
    "                  r'<a>\\1>' , text)\n",
    "\n",
    "\n",
    "s = '<a ddd href=\"1\" aaa ></a><a href=\"2\" aaa >ффф</a>'\n",
    "s2 = clear_tagparams(s)\n",
    "print(s2)"
   ]
  }
