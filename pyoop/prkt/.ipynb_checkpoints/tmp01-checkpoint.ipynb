    "import sys\n",
    "def getInputCod():\n",
    "    if \"idlelib\" in sys.modules:\n",
    "        input_cod ='cp1251'\n",
    "    if \"tkinter\" in sys.modules:\n",
    "        input_cod ='cp1251'\n",
    "    elif sys.stdin.isatty():\n",
    "        input_cod ='cp866'\n",
    "    else:\n",
    "        input_cod ='cp1251'\n",
    "    return input_cod\n",
    "#------------------------------------------\n",
    "\n",
    "def absNum(num,count):\n",
    "    if num<0: \n",
    "        num += count + 1\n",
    "    if num<1 or num>count: \n",
    "        return 1\n",
    "    return num\n",
    "        \n",
    "#-- list class with natural index and pagination\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
