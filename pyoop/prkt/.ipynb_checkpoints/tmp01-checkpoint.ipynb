    "\n",
    "\n",
    "\n",
    "\n",
    "#40\n",
    "class nlist(list):\n",
    "    def __init__(self, ls, ch_size = 0, file_cod = 'cp1251'):\n",
    "        self.chunk_size = abs(ch_size)\n",
    "        self.chunk_num = 1\n",
    "        self.fh = 0\n",
    "        self.input_cod = getInputCod()\n",
    "        self.file_cod = file_cod\n",
    "        if type(ls) == str:\n",
    "            if not file_cod:\n",
    "                self.file_cod = getFileCod(ls)\n",
    "                \n",
    "            self.fh = open(ls, 'rb+')\n",
    "            ls = [0]; offset = 0\n",
    "            while True:\n",
    "                line = self.fh.readline()\n",
    "                if not line:      #if line == '':\n",
    "                    break\n",
    "                offset = self.fh.tell()\n",
    "                ls.append(offset)\n",
    "                \n",
    "            self.fh.seek(offset)\n",
    "            st = self.fh.readline().rstrip()\n",
    "            print(st,len(st))\n",
    "            if len(st) < 1: ls.pop()\n",
    "            self.fh.seek(0,2)\n",
    "            self.fh.write('\\r\\n')\n",
    "                \n",
    "            list.__init__(self, ls)\n",
    "        else:\n",
    "            for j,val in enumerate(ls):\n",
    "                if type(val)==str:\n",
    "                    ls[j] = val.decode(self.input_cod)\n",
    "                list.__init__(self,ls)    \n",
    "\n",
    "    def __getitem__(self, key):\n",
    "        if type(key) == int:\n",
    "            key = absNum(key, len(self))\n",
    "            if key == 0: return list.__getitem__(self, len(self))\n",
    "            el = list.__getitem__(self, key-1)\n",
    "            if self.fh:\n",
    "                self.fh.seek(el)\n",
    "                el = self.fh.readline().decode(self.file_cod).rstrip()\n",
    "            try:\n",
    "                el = el.encode(self.enput_cod)\n",
    "            except:\n",
    "                pass\n",
    "        return el\n",
    "\n",
    "    def __setitem__(self, key, value):\n",
    "        if type(key) == int:\n",
    "            key = absNum(key, len(self))\n",
    "            if key == 0: list.__setitem__(self, len(self),value)\n",
    "            value = value.decode(self.input_cod)\n",
    "            if self.fh:\n",
    "                self.fh.seek(0,2)\n",
    "                offset = self.fh.tell()\n",
    "                value = value + '\\r\\n'\n",
    "                value = value.encode(self.file_cod)\n",
    "                self.fh.write(value)\n",
    "                value = offset\n",
    "            list.__setitem__(self, key-1, value)\n",
    "\n",
    "    def append(self, value):\n",
    "        value=value.decode(self.input_cod)\n",
    "        if self.fh:\n",
    "            self.fh.seek(0,2)\n",
    "            offset = self.fh.tell()\n",
    "            value = value + '\\r\\n'\n",
    "            #if self.input=cod:\n",
    "            value = value.encode(self.file_cod)\n",
    "            self.fh.write(value)\n",
    "            value = offset\n",
    "        list.append(self,value)\n",
    "            \n",
    "    def __delitem__(self, key):\n",
    "        if type(key) == int:\n",
    "            key = absNum(key, len(self))\n",
    "            if key == 0: list.__delitem__(self, len(self))\n",
    "            list.__delitem__(self, key - 1)\n",
    "\n",
    "    def getchunkNum(self):\n",
    "        return self.chunk_num\n",
    "    \n",
    "    def getChunkSize(self):\n",
    "        return self.chunk_size if self.chunk_size >0 else len(self)\n",
    "                                    \n",
    "    def setChunkSize(ch_size):\n",
    "        self.chunk_size = abs(ch_size)\n",
    "        \n",
    "    def countChunk(self):\n",
    "        if self.chunk_size < 1:  return 1\n",
    "        return (len(self) + self.chunk_size -1)//self.chunk_size\n",
    "        \n",
    "    def getChunk(self, num=0, step=0, reverse=0):\n",
    "        if len(self) == 0: return nlist([])\n",
    "        \n",
    "        if self.chunk_size>0:\n",
    "            chunk_size = self.chunk_size\n",
    "            chunk_count =(len(self) + chunk_size - 1)//chunk_size\n",
    "        else:\n",
    "            chunk_count=1\n",
    "            chunk_size = len(self)\n",
    "        \n",
    "        if num == 0: num= self.chunk_num\n",
    "        num += step\n",
    "        if num <0: num+= chunk_count + 1\n",
    "        \n",
    "        if num <1: \n",
    "            self.chunk_num = 1\n",
    "        elif num > chunk_count:\n",
    "            self.chunk_num = chunk_count\n",
    "        else:\n",
    "            self.chunk_num = num\n",
    "            \n",
    "        if reverse:\n",
    "            el_num=len(self) - (self.chunk_num-1)*chunk_size\n",
    "            chunk = [self[j] for j in range(el_num,el_num - chunk_size, -1) if j>0]\n",
    "        else:\n",
    "            el_num=(self.chunk_num-1)*chunk_size            \n",
    "            chunk = self[el_num:el_num + chunk_size]\n",
    "\n",
    "        if self.fh:\n",
    "            for j in range(len(chunk)):\n",
    "                el=chunk[j]\n",
    "                self.fh.seek(el)\n",
    "                chunk[j] = self.fh.readline().decode(self.file_cod)\n",
    "                \n",
    "        return nlist(chunk)\n",
    "    \n",
    "    def save(self, file, enc='cp1251'):\n",
    "        fh_out =open(file,'wb')\n",
    "        for num in range(len(self)):\n",
    "            el=list.__getitem__(self.num)\n",
    "            if self.fh:\n",
    "                self.fh.seek(el)\n",
    "                el= self.fh.readline().decode(self.file_cod).rstrip()\n",
    "                el= el +'\\r\\n'\n",
    "                fh_out.write(el.encode(enc))\n",
    "            else:\n",
    "                try:\n",
    "                    el=set(el)\n",
    "                except:\n",
    "                    pass\n",
    "                fh_out.write(el.encode(enc)+'\\r\\n')\n",
    "        fh_out.close()\n",
    "        \n",
